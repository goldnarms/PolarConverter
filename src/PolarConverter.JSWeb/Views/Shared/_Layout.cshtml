<!DOCTYPE html>

<html>
<head prefix="og: http://ogp.me/ns# fb: http://ogp.me/ns/fb# article: http://ogp.me/ns/article#">
    <meta property="og:image" content="http://polarconverter.azurewebsites.net/content/images/bgBilde.jpg" />
    <meta property="og:site_name" content="PolarConverter" />
    <meta property="fb:app_id" content="128362510636193">
    <meta property="og:type" content="website">
    <meta property="og:description" content="Convert your Polar files to Endomondo and Strava compatible files.">
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/bundles/fileupload/angularjs/css")
    @Scripts.Render("~/bundles/modernizr")
    <script src="https://timezonedb.googlecode.com/files/timezonedb.js" type="text/javascript"></script>
    <!--[if lt IE 9]><script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script><![endif]-->
    <!--[if IE]><meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1"><![endif]-->
    <meta name="description" content="Convert your Polar files to Endomondo and Strava compatible files." />
</head>
<body data-ng-app="polarApp">
    @Html.Partial("Plugins/_FacebookSdk")
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header" style="padding-top: 10px; padding-bottom: 10px">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a href="~/">
                    <img src="~/Content/polarLogo01_small.png" height="40" />
                </a>
                @*@Html.ActionLink("PolarConverter", "Index", "Home", null, new { @class = "navbar-brand" })*@
            </div>
            <div class="navbar-collapse collapse">
                @*<ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Upload", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>*@
                @*@Html.Partial("_LoginPartial")*@
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - PolarConverter</p>
        </footer>
    </div>
    @Html.Partial("Plugins/_Analytics")
    @Scripts.Render("~/bundles/libraries")
    @Scripts.Render("~/bundles/fileupload/angularjs/js")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
